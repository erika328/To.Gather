<%= form_with(model: @event, local: true) do |f| %>

  <div class="title">
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>

  <div class="time">
    <%= f.label :start_time %>
    <%= f.datetime_field :start_time %>
  </div>

  <div class="time">
    <%= f.label :end_time %>
    <%= f.datetime_field :end_time %>
  </div>


  <div class="submit">
    <%= f.submit %>
  </div>

<% end %>